<h2>Details for <%= @user_name %></h2>

<h4>Email address:</h4>
<p><%= @email %></p>

<h4>Interests</h4>
<p><%= @interests.join(', ') %></p>

<ul>
  <% @users.keys.each do |name| %>
    <% unless @user_name == name %>
      <li><a href="/<%= name %>"><%= name %></a></li>
    <% end %>
  <% end %>
</ul>